<%= element_editor_for(element) do |el| -%>
  <%= el.edit :essay_superheader %>
  <%= el.edit :article_title %>
  <%= el.edit :intro_blurb %>
  <% element.contents.where(name: ['credit_title', 'credit_author']).each do |content| %>
    <%= render_essence_editor content %>
  <% end %>
  <p><%= render_new_content_link(element) %></p>
<%- end -%>
